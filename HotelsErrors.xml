  <Hotel Rating="4.2">
    <Name>Westin Tempe</Name>
    <Phone>480-968-8885</Phone>
    <Phone>800-937-8461</Phone>
    <Address Number="11">
      <Street>E 7th St</Street>
      <City>Tempe</City>
      <State>AZ</State>
      <Zip>85281</Zip>
      <NearestAirport>Sky Harbor</NearestAirport>
    </Address>
  </Hotel>

  <!-- ERROR 2: Missing required attribute "Number" in Address -->
